    </footer>

  </main>

  <button onclick="topFunction()" id="back-to-top" title="Go to top">▲</button>

  <script>
    // --- Quiz Functionality ---
    function revealAnswer() {
        const answerSpan = document.getElementById('quiz-answer');
        const revealButton = document.querySelector('.reveal-btn');
        answerSpan.style.display = 'inline';
        revealButton.style.display = 'none'; // Hide the button after click
        
        // Optionally highlight the correct option (D)
        const options = document.querySelectorAll('.quiz-option');
        if (options.length >= 4) {
            options[3].style.color = 'var(--matrix)';
            options[3].style.fontWeight = 'bold';
        }
    }
    
    // --- Back to Top Functionality ---
    let mybutton = document.getElementById("back-to-top");

    window.onscroll = function() {scrollFunction()};

    function scrollFunction() {
      if (document.body.scrollTop > 400 || document.documentElement.scrollTop > 400) {
        mybutton.style.display = "flex";
      } else {
        mybutton.style.display = "none";
      }
    }

    function topFunction() {
      document.body.scrollTop = 0;
      document.documentElement.scrollTop = 0;
    }
  </script>
</body>
</html>
